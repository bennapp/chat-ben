<div id="ytplayer"></div>
<script>
  var player;
  function onYouTubeIframeAPIReady() {
    player = new YT.Player('ytplayer', {
      height: '720',
      width: '1280',
      videoId: '<%= value %>',
      events: {
        'onReady': onPlayerReady
      }
    });
  }

  function onPlayerReady(event) {
    event.target.playVideo();
    event.target.setVolume(10);
  }
</script>